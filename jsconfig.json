{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "frontend/*": [
        "frontend/*"
      ],
      "src/*": [
        "frontend/src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "frontend/src/components/*"
      ],
      "layouts/*": [
        "frontend/src/layouts/*"
      ],
      "pages/*": [
        "frontend/src/pages/*"
      ],
      "assets/*": [
        "frontend/src/assets/*"
      ],
      "boot/*": [
        "frontend/src/boot/*"
      ],
      "stores/*": [
        "frontend/src/stores/*"
      ],
      "vue$": [
        "node_modules/vue/dist/vue.runtime.esm-bundler.js"
      ],
      "backend/*": [
        "backend/*"
      ]
    }
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules"
  ]
}
